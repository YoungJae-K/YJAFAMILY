<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.yja.dao.model.GalleryPoto" table="GALYPOTO">
		<id name="galleryPhotoId" type="int">
			<column name="GALYPRID" not-null="true"/>			
		</id>		
		<property generated="never" lazy="false" name="galleryPhotoName" type="string">
			<column name="GALYFLNM" not-null="true"/>
		</property>	
		<property generated="never" lazy="false" name="insertDate" type="date">
			<column name="GALYINDT" not-null="true"/>
		</property>
		<property generated="never" lazy="false" name="updateDate" type="date">
			<column name="GALYUPDT" not-null="true"/>		
		</property>
	</class>
</hibernate-mapping>
